{
  "$schema": "https://vega.github.io/schema/vega-lite/v6.json",
  "data": {
    "url": "thresholds.csv"
  },
  "mark": "bar",
  "encoding": {
    "x": {
      "field": "Type",
      "type": "nominal",
      "sort": null
    },
    "y": {
      "field": "Count",
      "type": "quantitative"
    }
  },
  "transform": [
    {"filter": "datum.Threshold == 95 && datum.Size == 128"}
  ]
}

